getgenv().AutoHoldWeapon = false
getgenv().SelectWeapon = "Melee"

-- ===============================
-- SERVICES
-- ===============================
local Players = game:GetService("Players")
local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()

-- ===============================
-- CHARACTER (‡∏Å‡∏±‡∏ô‡∏ï‡∏≤‡∏¢/‡πÄ‡∏Å‡∏¥‡∏î‡πÉ‡∏´‡∏°‡πà)
-- ===============================
Player.CharacterAdded:Connect(function(char)
    Character = char
    task.wait(0.3)

    if getgenv().AutoHoldWeapon then
        EquipWeapon(getgenv().SelectWeapon)
    end
end)

-- ===============================
-- FUNCTION : ‡∏ñ‡∏∑‡∏≠ Tool ‡∏à‡∏£‡∏¥‡∏á
-- ===============================
function EquipWeapon(weaponType)
    if not weaponType or not Character then return end

    local Backpack = Player:WaitForChild("Backpack")

    -- ‡∏ñ‡∏≠‡∏î Tool ‡πÄ‡∏î‡∏¥‡∏°
    for _,v in pairs(Character:GetChildren()) do
        if v:IsA("Tool") then
            v.Parent = Backpack
        end
    end

    -- ‡∏´‡∏≤ Tool ‡∏ï‡∏≤‡∏° ToolTip
    for _,tool in pairs(Backpack:GetChildren()) do
        if tool:IsA("Tool") and tool.ToolTip == weaponType then
            tool.Parent = Character
            return
        end
    end
end

-- ===============================
-- CHECK HOLD WEAPON
-- ===============================
local function IsHoldingWeapon(weaponType)
    if not Character then return false end
    for _,v in pairs(Character:GetChildren()) do
        if v:IsA("Tool") and v.ToolTip == weaponType then
            return true
        end
    end
    return false
end

Tab:Toggle({
    Title = "Auto Hold Weapon",
    Desc = "",
    Value = false,
    Callback = function(v)
        getgenv().AutoHoldWeapon = v

        if v then
            EquipWeapon(getgenv().SelectWeapon)
        end
    end
})

