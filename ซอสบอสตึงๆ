local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local Workspace = game:GetService("Workspace")
local CollectionService = game:GetService("CollectionService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

local Player = Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()
local HRP = Character:WaitForChild("HumanoidRootPart")

local HOVER_HEIGHT = 40
local FLY_SPEED = 300

local TARGET_MOBS = {
    ["Reborn Skeleton"] = true,
    ["Posessed Mummy"] = true,
    ["Living Zombie"] = true
}

--==================================================
-- UPDATE CHARACTER (‡∏Å‡∏±‡∏ô‡∏ï‡∏≤‡∏¢)
--==================================================
local function UpdateCharacter()
    Character = Player.Character or Player.CharacterAdded:Wait()
    HRP = Character:WaitForChild("HumanoidRootPart")
end

Player.CharacterAdded:Connect(function()
    task.wait(0.3)
    UpdateCharacter()
end)

--==================================================
-- FLY
--==================================================
local function flyTo(cf)
    local dist = (HRP.Position - cf.Position).Magnitude
    local time = dist / FLY_SPEED

    local tween = TweenService:Create(
        HRP,
        TweenInfo.new(time, Enum.EasingStyle.Linear),
        {CFrame = cf}
    )
    tween:Play()
    tween.Completed:Wait()
end

--==================================================
-- FIND TARGET MOBS
--==================================================
local function getAllTargets()
    local list = {}

    for _, v in ipairs(Workspace:GetDescendants()) do
        if v:IsA("Model") and TARGET_MOBS[v.Name] then
            local hum = v:FindFirstChildOfClass("Humanoid")
            local root = v:FindFirstChild("HumanoidRootPart")
            if hum and root and hum.Health > 0 then
                table.insert(list, v)
            end
        end
    end

    return list
end

local function getNearestTarget()
    local nearest, shortest = nil, math.huge

    for _, mob in ipairs(getAllTargets()) do
        local dist = (HRP.Position - mob.HumanoidRootPart.Position).Magnitude
        if dist < shortest then
            shortest = dist
            nearest = mob
        end
    end

    return nearest
end

--==================================================
-- FAST ATTACK
--==================================================
assert(getrenv, "Exploit not supported")

local registerHit = debug.getupvalue(getrenv()._G.SendHitsToServer, 1)
local registerAttack = ReplicatedStorage.Modules.Net["RE/RegisterAttack"]

local function GetClosestEnemies()
    local enemies = CollectionService:GetTagged("BasicMob")
    local playerPos = HRP.Position
    local valid = {}

    for _, mob in pairs(enemies) do
        if TARGET_MOBS[mob.Name] then
            local hum = mob:FindFirstChildOfClass("Humanoid")
            local root = mob:FindFirstChild("HumanoidRootPart")
            if hum and root and hum.Health > 0 then
                local dist = (root.Position - playerPos).Magnitude
                if dist <= 120 then
                    table.insert(valid, {mob = mob, distance = dist})
                end
            end
        end
    end

    table.sort(valid, function(a,b)
        return a.distance < b.distance
    end)

    return #valid > 2 and {valid[1], valid[2]} or valid
end

local function Attack()
    if not getgenv().AutoFarm then return end

    local enemies = GetClosestEnemies()
    if not enemies or #enemies == 0 then return end

    local hitData = {}
    for _, enemy in pairs(enemies) do
        table.insert(hitData, {enemy.mob, enemy.mob.HumanoidRootPart})
    end

    registerAttack:FireServer(0/0)
    coroutine.resume(registerHit, hitData[1][2], {table.unpack(hitData, 2)})
end

--==================================================
-- ATTACK LOOP
--==================================================
task.spawn(function()
    while true do
        task.wait()
        if getgenv().AutoFarm then
            pcall(Attack)
        else
            task.wait(0.3)
        end
    end
end)

--==================================================
-- MAIN FARM LOOP
--==================================================
task.spawn(function()
    while true do
        task.wait(0.1)

        if not getgenv().AutoFarm then
            task.wait(0.5)
            continue
        end

        local target = getNearestTarget()
        if not target then
            task.wait(1)
            continue
        end

        local hum = target:FindFirstChildOfClass("Humanoid")
        local root = target:FindFirstChild("HumanoidRootPart")
        if not hum or not root then continue end

        flyTo(root.CFrame * CFrame.new(0, HOVER_HEIGHT, 0))

        while hum.Health > 0
        and HRP.Parent
        and getgenv().AutoFarm do
            HRP.CFrame = root.CFrame * CFrame.new(0, HOVER_HEIGHT, 0)
            task.wait(0.05)
        end
    end
end)

